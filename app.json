{
  "expo": {
    "name": "SPENDWISE",
    "slug": "spendwise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b47fcf41-9d9d-4cde-a7cc-e0a4e2126220"
      }
    },
    "android": {
      "package": "com.gayathri2003.spendwise"
    }
  }
}
